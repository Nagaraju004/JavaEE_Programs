<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>LOGIN2</display-name>


	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
	
	<servlet>
		<servlet-name>login</servlet-name>
		<servlet-class>test.Login</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	
	
	<servlet>
    	<servlet-name>Get_Data</servlet-name>
		<servlet-class>test.Get_Data</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>Get_Data</servlet-name>
		<url-pattern>/Get_Data</url-pattern>
	</servlet-mapping>
	
	
	<servlet>
		<servlet-name>out</servlet-name>
		<servlet-class>test.Logout</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>out</servlet-name>
		<url-pattern>/Logout</url-pattern>
	</servlet-mapping>


</web-app>